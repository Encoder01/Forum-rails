<%# form elementleri için eğer yanlış değer girilir ise messaj olarak gösterdik %>
<% flash.each do |type, message| %>
      <div data-alert class="alert-box">
        <%= message%>
        <a href="#" class="close">&times;</a>
      </div>
<%end%>
<%= content_for?(:content) ? yield(:content) : yield %>
